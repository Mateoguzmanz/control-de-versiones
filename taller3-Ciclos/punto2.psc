	Definir i,j,k Como Entero;
	Definir Aste Como Caracter;
	i <- 10;
	Aste <- "*";
		
	Mientras i >= 0 Hacer
		j <- 1;
		Mientras j <= i hacer
			
			Escribir " " ,Sin Saltar "";
			j<-j+1;
		FinMientras
		
		k <- 10;
		Mientras k >= i Hacer
			Escribir  "*" Sin Saltar;
			k<-k-1;
