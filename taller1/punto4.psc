Proceso sin_titulo
	Definir Ciudad Como Caracter;
	Definir Pais Como Caracter;
	
	Escribir "Nombre de la ciudad capital";
	Leer Ciudad;
	
	Escribir "Nombre del pais al que pertenece la ciudad capital";
	Leer Pais;
	
	Limpiar Pantalla;
	
	Escribir "La ciudad ", Ciudad, " es la capital del país ", Pais;
FinProceso
